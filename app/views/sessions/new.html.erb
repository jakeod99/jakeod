<html>

  <h1>Sign in</h1>

  <%= form_for(:session, url: signin_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
  <% end %>

  <p>New user? <%= link_to "Register now!", register_path %></p>

</html>